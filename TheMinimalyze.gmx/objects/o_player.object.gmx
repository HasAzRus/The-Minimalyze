        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Step
depth = y * -1;
ammo = clamp(ammo, 0, max_ammo);
life = clamp(life, 0, max_life);
//var
key_up = keyboard_check(ord("W"));
key_left = keyboard_check(ord("A"));
key_down = keyboard_check(ord("S"));
key_right = keyboard_check(ord("D"));
key_use = keyboard_check_pressed(ord("E"));
key_fire = mouse_check_button_pressed(mb_left);
key_alt_fire = mouse_check_button(mb_left);
key_fire2 = mouse_check_button_pressed(mb_right);
key_alt_fire2 = mouse_check_button(mb_right);
key_fire3 = mouse_check_button_pressed(mb_middle);
key_reload = keyboard_check_pressed(ord("R"));
key_weap_drop = keyboard_check_pressed(ord("Q"));
key_throw = keyboard_check_pressed(ord("F"));
key_use = keyboard_check_pressed(ord("E"));

key_sprint = keyboard_check(vk_shift);

key_walk = keyboard_check(vk_control);

key_dynamite = keyboard_check_pressed(ord('1'));
key_grenade = keyboard_check_pressed(ord('2'));
key_molotova = keyboard_check_pressed(ord('3'));
key_laser_mine = keyboard_check_pressed(ord('4'));

key_extinguish = keyboard_check_pressed(vk_space);

weap_dir = point_direction(x, y, mouse_x, mouse_y);

x_velocity = ((key_right)-(key_left));
y_velocity = ((key_up)-(key_down));

nearest_enemy = instance_nearest(x, y, o_zombies);

//weapons
weapons_inst[1] = o_pistol;
weapons_inst[2] = o_gun;
weapons_inst[3] = o_uzi;
weapons_inst[4] = o_ak47;
weapons_inst[5] = o_chainsaw;
weapons_inst[6] = o_machinegun;
weapons_inst[7] = o_mp5;
weapons_inst[8] = o_flamethrower;

//ui 
weapon_ui[1] = spr_pistol_pickup;
weapon_ui[2] = spr_gun_pickup;
weapon_ui[3] = spr_uzi_pickup;
weapon_ui[4] = spr_ak47_pickup;
weapon_ui[5] = spr_chainsaw_pickup;
weapon_ui[6] = spr_machinegun_pickup;
weapon_ui[7] = spr_mp5_pickup;
weapon_ui[8] = spr_flamethrower;

//movement
if can_move
{
    if key_up y -= spd;
    if key_left x -= spd;
    if key_down y += spd;
    if key_right x += spd;
}
    
//face to cursor
if x &gt; mouse_x image_xscale = -1;
else image_xscale = 1;

//health
if life &lt;= 0 
{
    instance_destroy();
}

//camera
if can_view
{
    view_xview = (x - view_wview / 2) + lengthdir_x(6, weap_dir);
    view_yview = (y - view_hview / 2) + lengthdir_y(6, weap_dir);
}

//view = (view_xview == (x - view_wview / 2) + lengthdir_x(6, weap_dir)) || (view_yview == (y - view_hview / 2) + lengthdir_y(6, weap_dir)); 

if(up &amp;&amp; !down)
{
    if(a &lt; 2 - ((2*life)/ 36))
    {
        a += 0.05;
        if(a &gt;= 2 - ((2*life)/ 36))
        {
            up = false;
            down = true;
        }  
    }
}

if(down &amp;&amp; !up)
{
    if(a &gt; -(2 - ((2*life)/ 36)))
    {
        a -= 0.05;
        if(a &lt;= -(2 - ((2*life)/ 36)))
        {
            up = true;
            down = false;
        }
    }
}
view_angle = a;
if key_alt_fire2
{
    alt_fire_spd = 2; 
    if view_wview &gt; 180 view_wview -= 5;
    if view_hview &gt; 180 view_hview -= 5;
}
else
{
    alt_fire_spd = 1;
    if view_wview &lt; 200 view_wview += 5;
    if view_hview &lt; 200 view_hview += 5;
}

if key_reload &amp;&amp; stock_ammo &gt; 0 &amp;&amp; ammo &lt; 18 reload_ammo();

//ammo
if ammo &lt;= 0 &amp;&amp; stock_ammo &gt; 0
{
    if stock_ammo &gt; max_ammo
    {
        //alarm[0] = reload_time;
        ammo += 18;
        stock_ammo -= 18;
    }
    else
    {
        //alarm[1] = reload_time;
        ammo += stock_ammo;
        stock_ammo = 0;
    }
}

//attack &amp; fire

if(!dont_punch &amp;&amp; can_attack) weapon_manager();

throw_weapon_manager();

if can_attack melee_weapon_manager();

if shooting &amp;&amp; set_weapon != noone 
{
    if(weapon_x == 0) weapon_x = -1;
    else if(weapon_x == -1) weapon_x = 0;
}

//burning
//if burning
//{   

//    alarm[1] = 
//}

burn_time = alarm[1];
if burning
{
    fire = instance_create(x + irandom_range(-4, 4), y + irandom_range(-4, 4), o_fire_part);
    player_damage(0.2);

    
    burn_time -= 1;
    
    if x_velocity != 0 &amp;&amp; y_velocity != 0 burn_time -= 10;
}

if burn_time &lt;= 0 burning = false;

if(armor_health &lt;= 0)
{
    if(armor != 0) armor = 0;
}

if(key_sprint &amp;&amp; can_sprint &amp;&amp; stamina &gt; 2)
{
    spd = 2;
    if(stamina &gt; 0) stamina -= 0.5;
}
else
{
    if(stamina &lt; 10) stamina += 0.2;
    spd = 1;
}

//if(key_walk)
//{
//    spd = 0.5;
//}
//if nearest_enemy != noone
//{
//    if distance_to_object(nearest_enemy) &lt; 3
//    {
//        if key_fire3
//        {
//            with(nearest_enemy)
//            {
//                if place_meeting(x + 4, y, other) impulseX(1, 4);
//                else if place_meeting(x - 4, y, other) impulseX(-1, 4);
                
//                if place_meeting(x, y + 4, other) impulseY(1, 4);
//                else if place_meeting(x, y - 4, other) impulseY(-1, 4);                
//            }
//        }
//    }
//}

//Altfire
alt_fire = key_alt_fire2;

</string>
          </argument>
        </arguments>
      </action>
