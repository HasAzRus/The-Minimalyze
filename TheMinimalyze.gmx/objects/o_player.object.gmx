    <event eventtype="4" ename="o_flamethrower">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Flamethrower Collision
if set_weapon == 8 &amp;&amp; other.can_take
{
    audio_play_sound(snd_take1, sp, false);
    if ammo &lt; max_ammo
    { 
        ammo += other.ammo;
        other.ammo = 0;
        
        if max_ammo &gt; other.ammo
            stock_ammo += (max_ammo - other.ammo) + other.stock_ammo;
        else
            stock_ammo += (other.ammo - max_ammo) + other.stock_ammo;
    }
    else
    {
        stock_ammo += other.ammo + other.stock_ammo;
    }
    
    fly_text("+" + string(other.ammo), c_white, x, y, 1, room_speed);
    with(other)  
        instance_destroy();
}
 
if key_use &amp;&amp; other.can_take
{
    audio_play_sound(snd_take1, sp, false);  
    
    fly_text("flamethrower",c_white, x, y, 1, room_speed);
    
    if set_weapon != 0
    {
        prev = instance_create(x + lengthdir_x(weap_drop_distance, -weap_dir), y + lengthdir_y(weap_drop_distance, -weap_dir), weapons_inst[set_weapon]);
        with(prev)
        {
            ammo = other.ammo;
            stock_ammo += other.stock_ammo;
        }
        
        set_weapon = 8;
                
        ammo = other.ammo;
        stock_ammo = other.stock_ammo;
    
        with(other)  
            instance_destroy();
    }
    else
    {
        set_weapon = 8;
        
        ammo = other.ammo;
        stock_ammo = other.stock_ammo;
    
        with(other)  
            instance_destroy();
    } 
}
</string>
          </argument>
        </arguments>
      </action>
