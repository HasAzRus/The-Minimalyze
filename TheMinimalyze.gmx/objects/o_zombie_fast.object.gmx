if target != noone
{
    target_x = target.x;
    target_y = target.y;
    
    dir = point_direction(x, y, target_x, target_y);
    
    target = instance_nearest(x, y, o_player);

    
    //face to target
    if x &gt; target_x image_xscale = -1;
    else image_xscale = 1;
    
    //movement to target
